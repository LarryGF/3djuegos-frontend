    "webpack": "^4.36.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.1.4",
    "dotenv": "^8.2.0",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.19.1",
